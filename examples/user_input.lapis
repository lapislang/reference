import <std/io.hl> // import

fn i32 main() {
  let n: i32; // value-less declaration
  stdin::scanf("%d", &n); // function call, referencing
  
  stdout::printf("The prime factors of %d are:\n", n);

  let div: i32 = 2;
  while (n > div) { // while loop
    if (n % div == 0) { // if statement
      n /= div; // /= assignment
      stdout::printf("%d", div);
    } else { // else statement
      if (div == 2) div = 3; // inline if statement
      else div += 2; // += assignment
    }
  }
}
